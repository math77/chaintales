pragma solidity ^0.8.0;


import "@openzeppelin/contracts/access/Ownable.sol";
import { Base64 } from "./libraries/Base64.sol";

interface IChainTalesMetadata {
	function tokenURI(uint256 tokenId, string memory origin, string memory sex) external view returns (string memory);
}

contract ChainTalesMetadata is Ownable, IChainTalesMetadata {

    bytes private pageLayout = "";

    constructor() Ownable() {

    }
	

	function tokenURI(uint256 tokenId, string memory origin, string memory sex) external view override returns (string memory) {

        string memory output = string(abi.encodePacked(
            "data:application/json;base64,eyJpbWFnZV9kYXRhIjogIjxzdmcgd2lkdGg9Jzg0MCcgaGVpZ2h0PScxMTQ3JyBmaWxsPSdub25lJyB4bWxucz0naHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmcnPiAg",
            pageLayout,
            "PC9zdmc+IiwgIm5hbWUiOiAiUHJvamVjdCBYIiwgImRlc2NyaXB0aW9uIjogIkNvb2wgYW5kIG1vZGVybiJ9"
            ));

        return output;

    }

    function toString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT license
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }
}